{
  "name": "--",
  "type": "module",
  "homepage": "https://github.com/Innei/bump-version",
  "bin": {},
  "version": "0.0.1",
  "files": [
    "dist",
    "package.json",
    "LICENSE"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --ignore-path ./.gitignore --write "
    ]
  },
  "scripts": {
    "dev": "NODE_ENV=development tsx src/main.mts",
    "prebuild": "rm -rf dist",
    "build": "tsc && ncc build lib/src/main.mjs -o dist",
    "test": "vitest"
  },
  "devDependencies": {
    "@innei/eslint-config-ts": "0.9.8",
    "@innei/prettier": "0.9.8",
    "@types/conventional-changelog": "3.1.1",
    "@types/inquirer": "9.0.3",
    "@types/semver": "7.3.13",
    "@vercel/ncc": "0.36.1",
    "husky": "8.0.3",
    "inquirer": "^9.2.0",
    "lint-staged": "13.1.4",
    "semver": "^7.5.0",
    "typescript": "5.0.4",
    "vite": "4.1.4",
    "vitest": "0.29.8",
    "zx": "^7.2.1"
  },
  "dependencies": {}
}
